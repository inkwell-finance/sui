#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/lars/sui/inkwell/node_modules/.pnpm/shx@0.4.0/node_modules/shx/lib/node_modules:/home/lars/sui/inkwell/node_modules/.pnpm/shx@0.4.0/node_modules/shx/node_modules:/home/lars/sui/inkwell/node_modules/.pnpm/shx@0.4.0/node_modules:/home/lars/sui/inkwell/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/lars/sui/inkwell/node_modules/.pnpm/shx@0.4.0/node_modules/shx/lib/node_modules:/home/lars/sui/inkwell/node_modules/.pnpm/shx@0.4.0/node_modules/shx/node_modules:/home/lars/sui/inkwell/node_modules/.pnpm/shx@0.4.0/node_modules:/home/lars/sui/inkwell/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../shx/lib/cli.js" "$@"
else
  exec node  "$basedir/../shx/lib/cli.js" "$@"
fi
